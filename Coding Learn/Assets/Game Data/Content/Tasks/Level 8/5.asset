    <TestCode>k__BackingField: "using System;\\nusing System.Collections.Generic;\\nusing
      System.Linq;\\n\\npublic class Level8Task1\\n{\\n//<playerCode>\\n    public
      string[] Palindroms = new string[]\\n    {\\n        \\\"\u043B\u0451\u0448\u0430\u043D\u0430\u043F\u043E\u043B\u043A\u0435\u043A\u043B\u043E\u043F\u0430\u043D\u0430\u0448\u0451\u043B\\\",\\n\\\"\u0430\u0440\u043E\u0437\u0430\u0443\u043F\u0430\u043B\u0430\u043D\u0430\u043B\u0430\u043F\u0443\u0430\u0437\u043E\u0440\u0430\\\",\\n\\\"\u0430\u0440\u0433\u0435\u043D\u0442\u0438\u043D\u0430\u043C\u0430\u043D\u0438\u0442\u043D\u0435\u0433\u0440\u0430\\\",\\n\\\"\u044F\u0438\u0434\u0443\u0441\u044A\u043C\u0435\u0447\u0435\u043C\u044A\u0441\u0443\u0434\u0438\u044F\\\",\\n\\\"\u044F\u0430\u0440\u043A\u0430\u043A\u0440\u0430\u044F\\\",\\n\\\"\u043C\u043E\u043B\u0435\u0431\u0435\u043D\u043E\u043A\u043E\u043D\u0435\u0431\u0435\u043B\u043E\u043C\\\"\\n   
      };\\n\\n    public string[] Words = new string[]\\n    {\\n        \\\"\u044F\u0431\u043B\u043E\u043A\u043E\\\",\\n       
      \\\"\u0443\u0440\u0444\u0443\\\",\\n        \\\"\u043F\u0440\u0435\u0437\u0438\u0434\u0435\u043D\u0442\\\"\\n   
      };\\n    \\n    public bool isTaskCompleted()\\n    {\\n        var random
      = new Random();\\n        return Check(random)&& Check(random) && Check(random)
      && Check(random) && Check(random) && Check(random) && Check(random);\\n   
      }\\n\\n    private bool Check(Random random)\\n    {\\n        var isPalindrom
      = random.Next(2) == 1;\\n        if (isPalindrom)\\n        {\\n           
      var word=Palindroms[random.Next(Palindroms.Length-1)];\\n            return
      Palindrom(word);\\n        }\\n        else\\n        {\\n            var word
      =Words[random.Next(Words.Length - 1)];\\n            return !Palindrom(word);\\n       
      }\\n    } \\n}"
