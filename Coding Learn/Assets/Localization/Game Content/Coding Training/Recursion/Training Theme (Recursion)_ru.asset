      \u043A \u0431\u0430\u0437\u043E\u0432\u043E\u043C\u0443 \u0441\u043B\u0443\u0447\u0430\u044E,
      \u0442\u0430\u043A \u043A\u0430\u043A \u043C\u044B \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0438\u043B\u0438
      \u0435\u0433\u043E \u0440\u0430\u0432\u043D\u044B\u043C \u0435\u0434\u0438\u043D\u0438\u0446\u0435.\n
      \n \u0422\u0430\u043A\u0438\u043C \u043E\u0431\u0440\u0430\u0437\u043E\u043C
      \u0441\u0442\u0435\u043A \u0432\u044B\u0437\u043E\u0432\u0430 \u0434\u043B\u044F
      \u0447\u0438\u0441\u043B\u0430 -5 \u0431\u0443\u0434\u0435\u0442 \u0432\u044B\u0433\u043B\u044F\u0434\u0435\u0442\u044C
      \u0442\u0430\u043A: Func(-5), Func(-6), Func(-7)... \u041E\u0442\u0441\u044E\u0434\u0430
      \u0432\u0438\u0434\u043D\u043E, \u0447\u0442\u043E \u0434\u043E \u0431\u0430\u0437\u043E\u0432\u043E\u0433\u043E
