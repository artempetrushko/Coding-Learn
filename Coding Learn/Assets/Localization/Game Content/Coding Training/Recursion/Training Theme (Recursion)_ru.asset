      \u0432\u0437\u0433\u043B\u044F\u0434, \u043D\u0438\u0447\u0435\u0433\u043E
      \u043D\u0435 \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u0441\u044F, \u0438
      \u0444\u0443\u043D\u043A\u0446\u0438\u044F \u0431\u0443\u0434\u0435\u0442 \u043F\u043E-\u043F\u0440\u0435\u0436\u043D\u0435\u043C\u0443
      \u043A\u043E\u0440\u0440\u0435\u043A\u0442\u043D\u043E \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C
